{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "hpreston/myhero_spark",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/myhero/spark",
    "instances": 1,
    "cpus": 0.5,
    "mem": 32,
    "env": {
        "myhero_app_server": "http://myhero-app",
        "myhero_app_key": "SecureApp",
        "myhero_spark_bot_email": "sparkuser@domain.com",
        "spark_token": "token",
        "myhero_spark_bot_url": "http://myhero-spark",
        "myhero_spark_bot_secret": "SecureBot"
    }
}